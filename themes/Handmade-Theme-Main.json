    "tokenColors": [
        {
            "name": "comments",
            "scope": [
                "comment",
                "comment.block",
                "comment.line",
                "punctuation.definition.comment",
                "storage.type.annotation",
                "punctuation.definition.annotation"
            ],
            "settings": {
                "foreground": "#606060"
            }
        },
        {
            "name": "main text",
            "scope": [
                "source",
                "keyword.operator",
                "variable.parameter",
                "variable.other.property",
                "keyword.other.unit",
                "entity.other.attribute-name",
                "entity.name.type.interface",
                "variable.language.wildcard.java",
                "variable.other.definition",
                "variable.other.object",
                "keyword.operator.assignment",
                "entity.name.function.operator",
                "support.type.property-name",
                "entity.name.section.markdown",
                "markup.bold.markdown",
                "punctuation",
                "storage.modifier.reference",
                "storage.modifier.pointer"
            ],
            "settings": {
                "foreground": "#b99468"
            }
        },
        {
            "name": "numbers and strings",
            "scope": [
                "constant.language",
                "constant.numeric",
                "constant.character",
                "constant.other",
                "constant.regexp",
                "constant.rgb-value",
                "constant",
                "string.quoted.single",
                "string.quoted.double",
                "string.interpolated.matlab",
                "keyword.other.unit.suffix",
                "keyword.other.unit.hexadecimal"
            ],
            "settings": {
                "foreground": "#A0CC60"
            }
        },
        {
            "name": "keywords and types",
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.operator.new",
                "keyword.operator.delete",
                "keyword.operator.sizeof",
                "keyword.other.using",
                "keyword.other.operator",
                "storage.type",
                "storage.type.struct",
                "storage.type.primitive",
                "storage.type.template",
                "storage.type.class",
                "storage.type.modifier",
                "storage.modifier",
                "meta.return-type",
                "support.type",
                "support.class",
                "entity.name.namespace",
                "entity.other.attribute",
                "entity.name.class",
                "entity.name.type",
                "entity.name.scope-resolution",
                "entity.name.scope-resolution.function.call",
                "entity.other.inherited-class",
                "entity.name.operator",
                "variable.language.this",
                "variable.language",
                "punctuation.definition.list.begin.markdown",
                "storage.type.function",
                "support.other.namespace"
            ],
            "settings": {
                "foreground": "#be9632"
            }
        },
        {
            "name": "macros & imports",
            "scope": [
                "meta.import",
                "meta.preprocessor.macro",
                "meta.preprocessor.include",
                "string.quoted.double.include",
                "string.quoted.other",
                "entity.name.function.preprocessor",
                "storage.modifier.import",
                "storage.modifier.package",
                "entity.name.import"
            ],
            "settings": {
                "foreground": "#1f7187"
            }
        },
        {
            "name": "functions",
            "scope": [
                "entity.name.function",
                "meta.function-call.generic",
                "support.function.builtin",
                "entity.name.function.call",
                "support.function.any-method",
                "support.function.go",
                "support.function.swift"
            ],
            "settings": {
                "foreground": "#b65f44"
            }
        }
    ]
}